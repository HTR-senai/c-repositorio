using system;

public class Program
{
    static int fatorial(int n)
    {
        if (n <= 1)
        {
            return 1;
        }
        return n * fatorial(n - 1);
    }

    public static void Main()
    (
        int numero = 5;
        int resultado = fatorial(nimero);
        Console.WriteLine($"O fatorial de {numero} Ã© {resultado}");


    )
}
